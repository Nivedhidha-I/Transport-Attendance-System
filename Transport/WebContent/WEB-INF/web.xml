<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Transport</display-name>
  
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>com.transport.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/log_in</url-pattern>
  </servlet-mapping>
  
  <servlet> 
  <servlet-name>edit</servlet-name>
  <servlet-class>com.transport.Student</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>edit</servlet-name>
  <url-pattern>/Student </url-pattern>
  </servlet-mapping>
  
  <servlet> 
  <servlet-name>save_edit</servlet-name>
  <servlet-class>com.transport.EditInfo</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>save_edit</servlet-name>
  <url-pattern>/EditInfo</url-pattern>
  </servlet-mapping>
  
  <servlet> 
  <servlet-name>insert</servlet-name>
  <servlet-class>com.transport.InsertAcc</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>insert</servlet-name>
  <url-pattern>/insertion</url-pattern>
  </servlet-mapping>
  
  <servlet> 
  <servlet-name>delete</servlet-name>
  <servlet-class>com.transport.DeleteAcc</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>delete</servlet-name>
  <url-pattern>/deletion</url-pattern>
  </servlet-mapping>
  
  <servlet> 
  <servlet-name>feedback</servlet-name>
  <servlet-class>com.transport.Feedback</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>feedback</servlet-name>
  <url-pattern>/Feedback</url-pattern>
  </servlet-mapping>
  
</web-app>